<!--OBJECT | HEADER-->
<header>
    <div>
        <a href="/">
            <img src="{{ template_directory }}/assets/tartine/rsc/img/img_logo.png" alt="logo">
        </a>

        <!--OBJECT | NAV-->
        <nav>

            <!--home-->
            {% include 'header/common/_brand.html.twig' with {'active': true} %}
            <!--search | visible on mobile/tablet && !homepage-extralarge-->
            {% include 'header/common/_search_screen.html.twig' with {'class': 'd-xl-none'} %}
            <!--all-->
            <div class="all d-ls-flex align-items-ls-center justify-content-ls-center">
                <a href="#" class="icon_custom position-relative">
                    <i class="icon_nav i-bars"></i>
                    <i class="i-bars i-dark-primary"></i>
                    <span>Vivre à Marche</span>
                    <!--<input type="radio" name="subnav" value="1" class="w-100 h-100">-->
                </a>

                <!--subnav-->
                {% include 'header/home/_subnav.html.twig' %}
            </div>
            <!--calendar-->
            <div class="calendar">
                <a href="#">
                    <i class="icon_nav i-clock"></i>
                    <span>Horaires-Contacts</span>
                </a>
            </div>
            <!--more-->
            <div class="more">
                <input type="checkbox" id="more" name="more"/>
                <a href="#" for="more">
                    <i class="icon_nav i-plus"></i>
                </a>

                <div class="bubble">
                    <i class="graphicElement"></i>
                    <div>
                        <a href="https://marche.guichet-citoyen.be/">E-Guichet</a>
                        <a href="/actualites">Actualités</a>
                        <a href="/tourisme/agenda-des-manifestations">Agenda</a>
                    </div>
                    <i class="graphicElement"></i>
                </div>
            </div>

            <!--visible from extralarge-->
            <!--e-ticket-->
            <div class="eticket">
                <a href="https://marche.guichet-citoyen.be/">
                    <span>E-Guichet</span>
                </a>
            </div>
            <!--news-->
            <div class="news">
                <a href="/actualites">
                    <span>Actualités</span>
                </a>
            </div>
            <!--agenda-->
            <div class="agenda">
                <a href="/tourisme/agenda-des-manifestations">
                    <span>Agenda</span>
                </a>
            </div>
        </nav>

        <div>
            <a href="#">
                <i class="fab fa-facebook-f"></i>
            </a>
            <a href="#">
                <i class="fab fa-twitter"></i>
            </a>
            <a href="/wp-content/themes/marchebe/assets/tartine" title="{{ showTemplate() }}">
                <i class="fab fa-instagram"></i>
            </a>
            <a href="#">
                <i class="fab fa-youtube"></i>
            </a>
        </div>
    </div>
</header>
